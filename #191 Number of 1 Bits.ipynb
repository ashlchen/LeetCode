    "class Solution:\n",
    "    def hammingWeight(self, n: int) -> int:\n",
    "        count_ones = 0\n",
    "        for i in range(32):\n",
    "            if n&1 ==1:\n",
    "                count_ones +=1\n",
    "            n >>= 1\n",
    "        return count_ones"
   ]
